        <assemblyIdentity name="PostSharp" publicKeyToken="b13fd38b8f9c99d7" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.3.18.0" newVersion="4.3.18.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
